  "scripts": {
    "migration": "knex migrate:latest --knexfile server/database/knexfile.js",
    "migration-dev": "cross-env NODE_ENV=development knex migrate:latest --knexfile server/database/knexfile.js",
    "start": "cross-env NODE_ENV=production node build",
    "build-client": "cross-env NODE_ENV=production webpack -p --config webpack.production.config",
    "webpack-stats": "cross-env NODE_ENV=production webpack -p --config webpack.production.config --json > stats.json",
    "build-server": "cross-env NODE_ENV=production babel server -d build",
    "dev": "cross-env NODE_ENV=development node -r dotenv/config -r babel-register server",
    "postinstall": "npm run migration && npm run build-client && npm run build-server"
